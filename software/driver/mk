#sudo  yum install kernel-tools
cpupower -c all frequency-set -g performance
sudo rmmod pcietest
make clean
make
sudo make install
sudo modprobe pcietest
# link mode = 13, length=4
echo 13,4 >/dev/pcietest/0
head -4 /dev/pcietest/0
./user
# link mode = 13, length=64
echo 13,64 >/dev/pcietest/0
head -4 /dev/pcietest/0
./user
#
cpupower -c all frequency-set -g ondemand
